      "sourceRoot": "apps/michi-processing-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/michi-processing-service/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "moralis": {
      "type": "library",
      "root": "libs/moralis",
