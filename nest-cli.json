{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/michi-api-service/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/michi-api-service/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/michi-api-service",
  "projects": {
    "michi-api-service": {
      "type": "application",
      "root": "apps/michi-api-service",
      "entryFile": "main",
      "sourceRoot": "apps/michi-api-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/michi-api-service/tsconfig.app.json"
      }
    },
    "michi-processing-service": {
